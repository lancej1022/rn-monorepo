{
  "name": "base-component-lib",
  "version": "0.1.0",
  "private": true,
  "description": "Base component library that wraps react-native-reusables",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./*": {
      "types": "./dist/*/index.d.ts",
      "require": "./dist/*/index.cjs",
      "default": "./dist/*/index.js"
    },
    "./cn": {
      "types": "./dist/utils/cn.d.ts",
      "require": "./dist/utils/cn.cjs",
      "default": "./dist/utils/cn.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "build-storybook": "storybook build",
    "clean": "bun run rm -rf dist storybook-static *.tsbuildinfo",
    "dev": "vite build -w",
    "lint": "eslint .  --max-warnings 0",
    "storybook": "storybook dev -p 6006 --no-open",
    "test-storybook": "test-storybook",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@rn-primitives/accordion": "~1.1.0",
    "@rn-primitives/alert-dialog": "~1.1.0",
    "@rn-primitives/aspect-ratio": "~1.1.0",
    "@rn-primitives/avatar": "^1.1.0",
    "@rn-primitives/checkbox": "~1.1.0",
    "@rn-primitives/collapsible": "~1.1.0",
    "@rn-primitives/context-menu": "~1.1.0",
    "@rn-primitives/dialog": "~1.1.0",
    "@rn-primitives/dropdown-menu": "~1.1.0",
    "@rn-primitives/hover-card": "~1.1.0",
    "@rn-primitives/label": "~1.1.0",
    "@rn-primitives/menubar": "~1.1.0",
    "@rn-primitives/navigation-menu": "~1.1.0",
    "@rn-primitives/popover": "~1.1.0",
    "@rn-primitives/progress": "~1.1.0",
    "@rn-primitives/radio-group": "~1.1.0",
    "@rn-primitives/select": "~1.1.0",
    "@rn-primitives/separator": "~1.1.0",
    "@rn-primitives/slot": "^1.1.0",
    "@rn-primitives/switch": "~1.1.0",
    "@rn-primitives/table": "~1.1.0",
    "@rn-primitives/tabs": "~1.1.0",
    "@rn-primitives/toggle": "~1.1.0",
    "@rn-primitives/toggle-group": "~1.1.0",
    "@rn-primitives/tooltip": "~1.1.0",
    "@rn-primitives/types": "^1.1.0",
    "@tanstack/react-table": "^8.11.7",
    "autoprefixer": "catalog:",
    "class-variance-authority": "catalog:",
    "clsx": "catalog:",
    "lucide-react": "catalog:",
    "lucide-react-native": "catalog:",
    "nativewind": "catalog:",
    "postcss": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-native": "catalog:",
    "react-native-reanimated": "catalog:",
    "react-native-web": "catalog:",
    "tailwind-merge": "catalog:",
    "tailwindcss": "catalog:",
    "tailwindcss-animate": "catalog:"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@eslint/js": "^9.13.0",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@storybook/react": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@tanstack/react-router-devtools": "^1.114.25",
    "@tanstack/router-plugin": "^1.114.25",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@types/react-native": "^0.73.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "globby": "^14.0.2",
    "playwright": "^1.46.1",
    "postcss": "^8.4.44",
    "rollup": "^4.24.4",
    "rollup-plugin-preserve-directives": "^0.4.0",
    "storybook": "^8.2.9",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vite-plugin-babel": "^1.2.0",
    "vite-plugin-dts": "^4.1.0",
    "vite-plugin-external": "^4.3.1",
    "vite-plugin-no-bundle": "^4.0.0",
    "vite-plugin-react-native-web": "^1.1.2",
    "web-vitals": "^4.2.4"
  }
}
